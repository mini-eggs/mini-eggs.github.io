{
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.17.3"
  },
  "scripts": {
    "start": "npm-run-all -p start:client start:server",
    "start:client": "parcel watch client/main.ts --out-dir static",
    "start:server": "gin",
    "build": "parcel build client/main.ts --out-dir static --detailed-report",
    "clean": "rm -rf static"
  },
  "dependencies": {
    "highlight.js": "^9.15.6",
    "marked": "^0.6.1",
    "turbolinks": "^5.2.0"
  }
}
